-- Scripts:

local function GMGB_fake_script() -- eon_reborn.mainlocal 
	local script = Instance.new('LocalScript', eon_reborn)

	
	
	script.Parent.main.page1.MoveButton.MouseButton1Click:Connect(function()
		if script.Parent.move.Visible == false then
			script.Parent.move.PokemonList.Visible = false
			script.Parent.move.MoveList.Visible = false
			script.Parent.move.MoveSearch.Visible = false
			script.Parent.move.MoveSearch.Text = "Search here!"
			script.Parent.move.MoveTable.Visible = true
			script.Parent.move["Current MoveList"].Visible = false
			script.Parent.main.page1.MoveButton.Text = "CLOSE"
			script.Parent.move.Visible = true
		else
			script.Parent.main.page1.MoveButton.Text = "OPEN"
			script.Parent.move.Visible = false
		end
	end)
	
	script.Parent.move.MinimizeSpawn.MouseButton1Click:Connect(function()
		script.Parent.move.PokemonList.Visible = false
		script.Parent.move.MoveList.Visible = false
		script.Parent.move.MoveSearch.Visible = false
		script.Parent.move.MoveSearch.Text = "Search here!"
		script.Parent.move.MoveTable.Visible = true
		script.Parent.move["Current MoveList"].Visible = false
		script.Parent.main.page1.MoveButton.Text = "OPEN"
		script.Parent.move.Visible = false
		
	end)
	
	script.Parent.main.page1.Minimize_1.MouseButton1Click:Connect(function()
		if script.Parent.showgui.Visible == false then
			script.Parent.move.PokemonList.Visible = false
			script.Parent.move.MoveList.Visible = false
			script.Parent.move.MoveSearch.Visible = false
			script.Parent.move.MoveSearch.Text = "Search here!"
			script.Parent.move.MoveTable.Visible = true
			script.Parent.move["Current MoveList"].Visible = false
			script.Parent.main.Visible = false
			
			script.Parent.main.page1.MoveButton.Text = "OPEN"
			script.Parent.move.Visible = false
			script.Parent.showgui.Visible = true
		end
	end)
	
	script.Parent.showgui.MouseButton1Click:Connect(function()
		if script.Parent.main.Visible == false then
			script.Parent.main.Visible = true
			script.Parent.showgui.Visible = false
		end
	end)
	
	
	
	
	
	
	for i, v in pairs(require(game.ReplicatedStorage.Information.Moves)) do 
	
		local textButton = Instance.new("TextButton")
		textButton.Parent = script.Parent.move.MoveList
		textButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		textButton.BorderColor3 = Color3.fromRGB(105, 105, 105)
		textButton.BorderSizePixel = 2
		textButton.Size = UDim2.new(0, 200, 0, 50)
		textButton.ZIndex = 112
		textButton.Font = Enum.Font.Arcade
		textButton.Text = i
		textButton.TextColor3 = Color3.fromRGB(84, 84, 84)
		textButton.TextScaled = true
		textButton.TextSize = 14.000
		textButton.TextWrapped = true
		textButton.MouseButton1Click:Connect(function()
			script.Parent.move.MoveTable.Text = textButton.Text
			script.Parent.move.MoveSearch.Visible = false
			script.Parent.move.MoveTable.Visible  = true
			script.Parent.move.MoveSearch.Text = "Search here!"
			script.Parent.move.MoveList.Visible = false
		end)
	
	end
	
	local FrameHolder = script.Parent.move.MoveList
	
	local function SortFrames()
		local frames = {}
	
		for i, child in ipairs(FrameHolder:GetChildren()) do
			if child:IsA("TextButton") then
				table.insert(frames, child)
			end
		end
	
		table.sort(frames, function(frame1, frame2)
			return frame1.Text:lower() < frame2.Text:lower()
		end)
	
		for i, sortedFrame in ipairs(frames) do
			sortedFrame.LayoutOrder = i
		end
	end
	local function visiblescroll()
	
	
		for z, x in ipairs(FrameHolder:GetChildren()) do
			if x:IsA("TextButton") then
				x.Visible = true
			end
		end
	
		
	end
	
	
	script.Parent.move.MoveTable.MouseButton1Click:Connect(function()
		if script.Parent.move.MoveList.Visible == false then
			SortFrames()
			visiblescroll()
			script.Parent.move.MoveList.CanvasSize = UDim2.new(0,0,0,script.Parent.move.MoveList.UIGridLayout.AbsoluteContentSize.Y)
			script.Parent.move.MoveTable.Visible  = false
			script.Parent.move.MoveSearch.Visible  = true
			script.Parent.move.MoveList.Visible  = true
		end
	end)
	
	script.Parent.move["Pokemon Name"].MouseButton1Click:Connect(function()
		if script.Parent.move.PokemonList.Visible == false then
			for u, p in pairs(script.Parent.move.PokemonList:GetChildren()) do
				if p.Name == "TextButton" then
					p:Remove()
				end
			end
			for h, k in pairs(game.Players.LocalPlayer.PokemonParty:GetChildren()) do 
				local textButton = Instance.new("TextButton")
				textButton.Parent = script.Parent.move.PokemonList
				textButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				textButton.BorderColor3 = Color3.fromRGB(105, 105, 105)
				textButton.BorderSizePixel = 2
				textButton.Size = UDim2.new(0, 200, 0, 50)
				textButton.ZIndex = 115
				textButton.Font = Enum.Font.Arcade
				textButton.Text = k.Name
				textButton.TextColor3 = Color3.fromRGB(84, 84, 84)
				textButton.TextScaled = true
				textButton.TextSize = 14.000
				textButton.TextWrapped = true
				script.Parent.move.PokemonList.CanvasSize = UDim2.new(0,0,0,script.Parent.move.PokemonList.UIGridLayout.AbsoluteContentSize.Y)
				textButton.MouseButton1Click:Connect(function()
					script.Parent.move["Pokemon Name"].Text = textButton.Text
					script.Parent.move.PokemonList.Visible = false
				end)
	
			end
			SortFrames()
			script.Parent.move.PokemonList.Visible = true
		else
			script.Parent.move.PokemonList.Visible = false
		end
	end)
	
	script.Parent.move.CurrentMove.MouseButton1Click:Connect(function()
		if script.Parent.move["Current MoveList"].Visible == false then
			for qq, ww in pairs(script.Parent.move["Current MoveList"]:GetChildren()) do
				if ww.Name == "TextButton" then
					ww:Remove()
				end
			end
			for jq, lq in pairs(game.Players.LocalPlayer.PokemonParty[script.Parent.move["Pokemon Name"].Text].Moves:GetChildren()) do
				local textButton = Instance.new("TextButton")
				textButton.Parent = script.Parent.move["Current MoveList"]
				textButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				textButton.BorderColor3 = Color3.fromRGB(105, 105, 105)
				textButton.BorderSizePixel = 2
				textButton.Size = UDim2.new(0, 200, 0, 50)
				textButton.ZIndex = 109
				textButton.Font = Enum.Font.Arcade
				textButton.Text = lq.Name
				textButton.TextColor3 = Color3.fromRGB(84, 84, 84)
				textButton.TextScaled = true
				textButton.TextSize = 14.000
				textButton.TextWrapped = true
				script.Parent.move["Current MoveList"].CanvasSize = UDim2.new(0,0,0,script.Parent.move["Current MoveList"].UIGridLayout.AbsoluteContentSize.Y)
				textButton.MouseButton1Click:Connect(function()
					script.Parent.move.CurrentMove.Text = textButton.Text
					script.Parent.move["Current MoveList"].Visible = false
				end)
			end
			SortFrames()
			script.Parent.move["Current MoveList"].Visible  = true
		else
			script.Parent.move["Current MoveList"].Visible  = false
		end
	end)
	
	
	local ScrollingFrame = script.Parent.move.MoveList
	local SearchBar = script.Parent.move.MoveSearch
	SearchBar:GetPropertyChangedSignal("Text"):Connect(function()
		local InputText = string.lower(SearchBar.Text)
		for _, Button in pairs(ScrollingFrame:GetChildren()) do
			if Button:IsA("TextButton")then
				Button.Visible = string.find(string.lower(Button.Text), InputText) and true or false
			end
		end
		
	end)
	
	
	
	
	script.Parent.move.RepleaceMove.MouseButton1Click:Connect(function()
			local v1 = game:GetService("Players").LocalPlayer.PokemonParty[script.Parent.move["Pokemon Name"].Text]
			local v2 = game:GetService("Players").LocalPlayer.PokemonParty[script.Parent.move["Pokemon Name"].Text].Moves[script.Parent.move["CurrentMove"].Text]
			local v3 = script.Parent.move["MoveTable"].Text
			local event = game:GetService("ReplicatedStorage").REvents.Pokemon.TMLearn
			event:InvokeServer(v1, v2, v3)
	end)
	
	
	script.Parent.main.page1.NextArrow_1.MouseButton1Click:Connect(function()
		script.Parent.main.page1.Visible = false
		script.Parent.main.page2.Visible = true
		script.Parent.move.PokemonList.Visible = false
		script.Parent.move.MoveList.Visible = false
		script.Parent.move.MoveSearch.Visible = false
		script.Parent.move.MoveSearch.Text = "Search here!"
		script.Parent.move.MoveTable.Visible = true
		script.Parent.move["Current MoveList"].Visible = false
		script.Parent.main.page1.MoveButton.Text = "OPEN"
		script.Parent.move.Visible = false
	end)
	script.Parent.main.page1.BackArrow_1.MouseButton1Click:Connect(function()
		script.Parent.main.page1.Visible = false
		script.Parent.main.page7.Visible = true
		script.Parent.move.PokemonList.Visible = false
		script.Parent.move.MoveList.Visible = false
		script.Parent.move.MoveSearch.Visible = false
		script.Parent.move.MoveSearch.Text = "Search here!"
		script.Parent.move.MoveTable.Visible = true
		script.Parent.move["Current MoveList"].Visible = false
		script.Parent.main.page1.MoveButton.Text = "OPEN"
		script.Parent.move.Visible = false
	end)
	script.Parent.main.page2.NextArrow_2.MouseButton1Click:Connect(function()
		script.Parent.main.page2.Visible = false
		script.Parent.main.page3.Visible = true
		script.Parent.move.PokemonList.Visible = false
		script.Parent.move.MoveList.Visible = false
		script.Parent.move.MoveSearch.Visible = false
		script.Parent.move.MoveSearch.Text = "Search here!"
		script.Parent.move.MoveTable.Visible = true
		script.Parent.move["Current MoveList"].Visible = false
		script.Parent.main.page1.MoveButton.Text = "OPEN"
		script.Parent.move.Visible = false
	end)
	script.Parent.main.page2.BackArrow_2.MouseButton1Click:Connect(function()
		script.Parent.main.page2.Visible = false
		script.Parent.main.page1.Visible = true
		script.Parent.move.PokemonList.Visible = false
		script.Parent.move.MoveList.Visible = false
		script.Parent.move.MoveSearch.Visible = false
		script.Parent.move.MoveSearch.Text = "Search here!"
		script.Parent.move.MoveTable.Visible = true
		script.Parent.move["Current MoveList"].Visible = false
		script.Parent.main.page1.MoveButton.Text = "OPEN"
		script.Parent.move.Visible = false
	end)
	
	
	
	
	
	
	script.Parent.main.page2.PlayerName.MouseButton1Click:Connect(function()
		
			for m, n in pairs(script.Parent.main.page2.PlayerList:GetChildren()) do
				if n.Name == "TextButton" then
					n:Remove()
				end
			end
		for N, M in pairs(game.Players:GetChildren()) do 
			if M.Name ~= game.Players.LocalPlayer.Name then
				local textButton = Instance.new("TextButton")
				textButton.Parent = script.Parent.main.page2.PlayerList
				textButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				textButton.BorderColor3 = Color3.fromRGB(105, 105, 105)
				textButton.BorderSizePixel = 2
				textButton.Size = UDim2.new(0, 154, 0, 21)
				textButton.ZIndex = 98
				textButton.Font = Enum.Font.Arcade
				textButton.Text = M.Name
				textButton.TextColor3 = Color3.fromRGB(84, 84, 84)
				textButton.TextScaled = true
				textButton.TextSize = 14.000
			    textButton.TextWrapped = true
			    script.Parent.main.page2.PlayerList.CanvasSize = UDim2.new(0,0,0,script.Parent.main.page2.PlayerList.UIGridLayout.AbsoluteContentSize.Y)
				textButton.MouseButton1Click:Connect(function()
				script.Parent.main.page2.PlayerName.Text = textButton.Text
				script.Parent.main.page2.PlayerList.Visible = false
				script.Parent.main.page2.PlayerName.Visible = true
				script.Parent.main.page2.PlayerTrading.Visible = false
					
				end)
	end
			end
		   SortFrames()
		    script.Parent.main.page2.PlayerList.CanvasSize = UDim2.new(0,0,0,script.Parent.main.page2.PlayerList.UIGridLayout.AbsoluteContentSize.Y)
			script.Parent.main.page2.PlayerList.Visible = true
			script.Parent.main.page2.PlayerName.Visible = false
			script.Parent.main.page2.PlayerTrading.Visible = true
	end)
	
	
	local ScrollingFrame = script.Parent.main.page2.PlayerList
	local SearchBar = script.Parent.main.page2.PlayerTrading
	SearchBar:GetPropertyChangedSignal("Text"):Connect(function()
		local InputText = string.lower(SearchBar.Text)
		for _, Button in pairs(ScrollingFrame:GetChildren()) do
			if Button:IsA("TextButton")then
				Button.Visible = string.find(string.lower(Button.Text), InputText) and true or false
			end
		end
	
	end)
	
	script.Parent.main.page2.Minimize_2.MouseButton1Click:Connect(function()
		if script.Parent.showgui.Visible == false then
			script.Parent.main.Visible = false
			script.Parent.main.page2.PlayerTrading.Visible = false
			script.Parent.main.page2.PlayerName.Visible = true
			script.Parent.main.page2.PlayerList.Visible = false
			script.Parent.showgui.Visible = true
		end
	end)
	
	script.Parent.main.page2.TradeButton.MouseButton1Click:Connect(function()
		if script.Parent.main.page2.PlayerName.Text ~= "Player Name" then
			local remote = game["Workspace"]["Remotes"]["Action"]
	
			local args = {
				[1] = "Accept",
				[2] = game["Players"][script.Parent.main.page2.PlayerName.Text]
			}
			remote:FireServer(unpack(args))
		end
	end)
end
coroutine.wrap(GMGB_fake_script)()
local function CHKZZ_fake_script() -- eon_reborn.LocalScript 
	local script = Instance.new('LocalScript', eon_reborn)

	startss = false
	moneyval = 0
	
	local getclosest = function()
		local ClosestObject
		for i, v in pairs(game:GetService("Players").LocalPlayer.PokemonParty:GetChildren()) do
			if v.PartyPosition.Value == 1 then
				ClosestObject = v.Name
			end
		end
		return ClosestObject
	end
	
	
	script.Parent.main.page3.OpenPCButton.MouseButton1Click:Connect(function()
		if script.Parent.main.page3.FindButton.Text == "Stop!" then
			getrenv()._G.PCSystem()
		end
	end)
	
	
	script.Parent.main.page3.FindButton.MouseButton1Click:Connect(function()
		if startss == false then
			script.Parent.main.page3.FindButton.Text = "Start!"
			script.Parent.main.page3.FindButton.BackgroundColor3 = Color3.fromRGB(84, 255, 126)
			startss = true
	
			
		else 
			script.Parent.main.page3.FindButton.Text = "Stop!"
			script.Parent.main.page3.FindButton.BackgroundColor3 = Color3.fromRGB(255, 89, 93)
			startss = false
			
			
		end
	end)
	
	spawn(function()
		while true do
			wait()
			if startss == true then
				pcall(function()
					getrenv()._G.HUNTCOMMAND = true
					repeat 
						local place =  workspace.WildAreas:GetChildren()
						local randomplace = place[math.random(1, #place)]
						local randomm = randomplace
						local currentPivot = randomm:GetPivot()
						local char = game.Players.LocalPlayer.Character
						local character = char.HumanoidRootPart
						character:PivotTo(currentPivot)
						local items = randomm:GetChildren()
						local randomItem = items[math.random(1, #items)]
	
						character:PivotTo(randomItem:GetPivot())
						char.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
	
						wait(1.5) until game:GetService("Players").LocalPlayer.PlayerGui.Main.BattleScene.Position.Y.Scale ~= -1
					local near = getclosest()
					repeat wait() until game:GetService("Players").LocalPlayer.PlayerGui.Main.BattleScene.dialogbg.dialog.dropshadow.Text == "What should "..near.." do?"
					local senv = getsenv(game.Players.LocalPlayer.PlayerGui["_G.Battle"])
					senv.capturedpokemon("Master Ball")
					for i,v in ipairs(game:GetService("Players").LocalPlayer.OppPokemon:GetChildren()) do
						senv.losehealth(100, game.Players.LocalPlayer.OppPokemon[v.Name], true, "Lolz")
					end
	
					repeat wait() until game:GetService("Players").LocalPlayer.PlayerGui.Main.BattleScene.ButtonHolder.Fight.Visible == true
	
					local vim = game:GetService'VirtualInputManager'
					local x = game:GetService("Players").LocalPlayer.PlayerGui.Main.BattleScene.ButtonHolder.Run
	
					vim:SendMouseButtonEvent(x.AbsolutePosition.X+x.AbsoluteSize.X/2,x.AbsolutePosition.Y+50,0,true,x,1)
					vim:SendMouseButtonEvent(x.AbsolutePosition.X+x.AbsoluteSize.X/2,x.AbsolutePosition.Y+50,0,false,x,1)
					getrenv()._G.HUNTCOMMAND = false
					repeat wait() until game:GetService("Players").LocalPlayer.PlayerGui.Main.BattleScene.Position.Y.Scale == -1
				end)
			end
		end
	end)
	
	
	
